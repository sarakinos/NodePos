<!-- views/profile.ejs -->
<!doctype html>
<html>
<head>
    <% include partials/head %>
</head>
<body>
<div class="container">

    <div class="page-header text-center">
        <h1><span class="fa fa-anchor"></span> Admini Page</h1>
        <a href="/logout" class="btn btn-default btn-sm">Logout</a>
    </div>

    <div class="row">

        <!-- LOCAL INFORMATION -->
        <div class="col-sm-6">
            <div class="well">
                <h3><span class="fa fa-user"></span> Local</h3>

                    <p>
                        <strong>id</strong>: <%= user._id %><br>
                        <strong>email</strong>: <%= user.local.email %><br>
                        <strong>firstname</strong>: <%= user.local.firstname %>
                        <strong>surname</strong>: <%= user.local.surname %>
                        <strong>addr</strong>: <%= user.local.addr %>
                        <strong>afm</strong>: <%= user.local.afm %>
                        <strong>am</strong>: <%= user.local.am %>
                        <strong>admin</strong>: <%= user.local.admin %>
                      
                    </p>

            </div>
        </div>

    </div>

</div>
<% include partials/footer %>
</body>
</html>